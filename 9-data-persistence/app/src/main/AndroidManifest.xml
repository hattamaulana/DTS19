<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="polinema.ac.id.dtsapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:name=".DTS"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.ProfileActivity"></activity>
        <activity android:name=".activity.HomeActivity" />
        <activity android:name=".activity.auth.RegisterActivity" />
        <activity android:name=".activity.SuccessActivity" />
        <activity android:name=".activity.auth.ResetPasswordActivity" />
        <activity android:name=".activity.auth.ForgotPasswordActivity" />
        <activity android:name=".activity.auth.WelcomeBackActivity" />
        <activity android:name=".WelcomeSlideAssignActivity" />
        <activity android:name=".WelcomeSlideSuperheroActivity" />
        <activity android:name=".WelcomeSlideCalendarActivity" />
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>